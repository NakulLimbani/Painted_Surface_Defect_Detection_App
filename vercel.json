{
    "builds": [
      {
        "src": "defect_detection_project/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "defect_detection_project/wsgi.py"
      }
    ]
  }
  